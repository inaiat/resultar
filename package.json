{
  "name": "resultar",
  "version": "1.2.0",
  "description": "Result pattern for typescript",
  "type": "module",
  "packageManager": "pnpm@9.7.1",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=20"
  },
  "outDir": "./dist",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && git rev-parse HEAD > BUILD_SHA",
    "format": "dprint fmt",
    "lint": "dprint check && eslint",
    "lint:fix": "dprint fmt && eslint --fix",
    "prepublishOnly": "pnpm run build",
    "test": "node --import=tsx/esm --test tests/*.ts",
    "typedoc": "typedoc",
    "prepare": "husky || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inaiat/resultar.git"
  },
  "keywords": [
    "plugin"
  ],
  "contributors": [
    "Inaiat <inaiat@gmail.com>",
    "Tomaz Haidar <tomaz_haidar@hotmail.com>"
  ],
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "bugs": {
    "url": "https://github.com/inaiat/resultar/issues"
  },
  "homepage": "https://github.com/inaiat/resultar",
  "devDependencies": {
    "@types/node": "22.4.0",
    "dprint": "0.47.2",
    "eslint": "^9.9.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-unicorn": "55.0.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "husky": "^9.1.4",
    "semver": "^7.6.3",
    "testdouble": "^3.20.2",
    "tsup": "^8.2.4",
    "tsx": "^4.17.0",
    "typedoc": "^0.26.6",
    "typedoc-github-wiki-theme": "2.0.0",
    "typedoc-plugin-markdown": "4.2.5",
    "typescript": "5.5.4",
    "typescript-eslint": "8.1.0"
  },
  "tsup": {
    "sourcemap": true,
    "dts": true,
    "clean": true,
    "bundle": true,
    "target": "node18",
    "format": [
      "esm",
      "cjs"
    ],
    "entry": [
      "src/**/*.ts"
    ]
  },
  "directories": {
    "test": "test"
  },
  "release": {
    "branches": [
      {
        "name": "main"
      },
      {
        "name": "beta",
        "channel": "beta",
        "prerelease": true
      },
      {
        "name": "next",
        "channel": "channel-next"
      }
    ]
  },
  "author": "inaiat@gmail.com"
}
